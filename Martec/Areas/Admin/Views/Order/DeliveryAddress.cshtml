@model Martec.Domain.Models.OrderDeliveryModel

@{
    ViewBag.Title = "DeliveryAddress";
}
@if (TempData["Message"] != null)
{
    <div class="alert alert-warning alert-dismissible fade in" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">x</span></button>@TempData["Message"]
    </div>
}
@if (Model != null)
{
    <h2>DeliveryAddress</h2>

    <table class="table table-striped">
        <thead>
            <tr>
                <th>Full Name</th>
                <th>Delivery Address</th>
                <th>Phone Number</th>
                <th>Created Date</th>

            </tr>
        </thead>
        <tbody>


            <tr>
                <td>@Model.FullName</td>
                <td>@Model.DeliveryAddress</td>
                <td>@Model.PhoneNumber </td>
                <td>@Model.CreatedDate</td>


            </tr>


        </tbody>
    </table>
}

