@model Martec.Domain.Models.OrderModel

@{
    ViewBag.Title = "Place";
}
@if (TempData["Message"] != null)
{
    <div class="alert alert-warning alert-dismissible fade in" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">x</span></button>@TempData["Message"]
    </div>
}
@if (Model != null)
{
    <div class="w3-container">
        <div class="w3-panel w3-border w3-light-grey w3-round-large">
            <p>
                Dear Esteem Customer, You Have Successfully Place An Order.
                You Will Be Contacted In The Next Few Minutes
            </p>
        </div>
    </div>
}

